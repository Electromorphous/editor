{
  "name": "@iooxa/schema",
  "version": "0.1.0",
  "description": "Schema and markdown parser for @iooxa/editor",
  "author": "Rowan Cockett <rowan@iooxa.com>",
  "homepage": "https://github.com/iooxa/schema",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iooxa/schema.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn run clean && tsc -p .;rm -rf dist/test; mv dist/src/* dist; rmdir dist/src;",
    "build:watch": "tsc -w -p .",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:clear": "jest --clearCache",
    "lint": "eslint \"src/**/*.ts\" -c .eslintrc.json"
  },
  "bugs": {
    "url": "https://github.com/iooxa/schema/issues"
  },
  "dependencies": {
    "markdown-it": "^12.0.2",
    "markdown-it-myst": "^0.0.7",
    "orderedmap": "^1.1.1",
    "prosemirror-collab": "^1.2.2",
    "prosemirror-markdown": "^1.5.0",
    "prosemirror-model": "^1.12.0",
    "prosemirror-schema-list": "^1.1.4",
    "prosemirror-state": "^1.3.3",
    "prosemirror-transform": "^1.2.8",
    "prosemirror-view": "^1.16.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@sinonjs/fake-timers": "^6.0.1",
    "@types/jest": "^26.0.16",
    "@types/jsdom": "^16.2.5",
    "@types/markdown-it": "^10.0.3",
    "@types/mocha": "^8.0.4",
    "@types/orderedmap": "^1.0.0",
    "@types/prosemirror-collab": "^1.1.0",
    "@types/prosemirror-markdown": "^1.0.3",
    "@types/prosemirror-model": "^1.11.1",
    "@types/prosemirror-schema-list": "^1.0.1",
    "@types/prosemirror-state": "^1.2.5",
    "@types/prosemirror-test-builder": "^1.0.1",
    "@types/prosemirror-transform": "^1.1.1",
    "@types/prosemirror-view": "^1.16.1",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-no-skip-tests": "^1.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "prosemirror-test-builder": "^1.0.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
}
